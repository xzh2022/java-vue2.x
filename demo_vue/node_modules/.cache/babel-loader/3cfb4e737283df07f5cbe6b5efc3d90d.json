r\n    delHandle(id) {\r\n\r\n      let ids = []\r\n      ids.push(id)\r\n\r\n      this.$axios.post(\"/sys/role/delete/\", ids).then(res => {\r\n\r\n        //弹窗 删除成功\r\n        this.$message({\r\n          showClose: true,\r\n          message: '恭喜你，删除成功',\r\n          type: 'success',\r\n          // 弹窗操作成功之后，进行一个回调，更新列表数据\r\n          onClose: () => {\r\n            this.getRoleList()\r\n          }\r\n        });\r\n      })\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n    .el-pagination {\r\n      float: right;\r\n      margin-top: 22px;\r\n    }\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}